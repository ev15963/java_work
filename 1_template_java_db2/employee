mysql> create table employee(
    -> empNo int auto_increment primary key,
    -> empName varchar(20) not null,
    -> job varchar(20) not null,
    -> mgr varchar(20) not null,
    -> hireDate varchar(20) not null,
    -> sale int,
    -> commission int,
    -> deptNo int
    -> );
